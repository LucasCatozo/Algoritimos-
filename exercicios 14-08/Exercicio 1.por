programa
{
    funcao inicio()
    {
        // Declaração de variáveis
        real comprimento, largura, area

        // Entrada de dados
        escreva("Digite o comprimento do retângulo: ")
        leia(comprimento)

        escreva("Digite a largura do retângulo: ")
        leia(largura)

        // Cálculo da área
        area = comprimento * largura

        // Saída do resultado
        escreva("Área = ", area)
    }
}
