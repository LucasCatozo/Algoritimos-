programa {
  funcao inicio() {
    // Entrada de dados
    real comprimento, largura
    escreva("Informe o comprimento: ")
    leia(comprimento)
    escreva("Informe a largura: ")
    leia(largura)
    //Processamento de dados
    real area = comprimento * largura
    // Saída de dados
    escreva("A área do retângulo é", area)
  }
}
